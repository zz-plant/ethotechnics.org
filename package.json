{
  "name": "et3",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "deploy": "wrangler deploy ./dist/_worker.js",
    "astro:check": "astro check",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,astro}\"",
    "check": "npm run lint --if-present && npm run test:ci --if-present && npm run typecheck --if-present && npm run astro:check",
    "test": "vitest",
    "test:ci": "vitest --run --coverage",
    "e2e": "npm run build && npx playwright test",
    "e2e:ci": "npm run build && npx playwright test --reporter=line"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/sitemap": "^3.6.0",
    "@fontsource-variable/plus-jakarta-sans": "^5.2.4",
    "@fontsource-variable/source-serif-4": "^5.2.9",
    "@iconify-json/lucide": "^1.2.77",
    "@iconify-json/tabler": "^1.2.23",
    "astro": "^5.16.3",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^27.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}
