---
export interface Props {
  title?: string;
  ariaHidden?: boolean;
  class?: string;
}

const {
  title = "Ethotechnics logo",
  ariaHidden = false,
  class: className,
} = Astro.props as Props;
---

<svg
  class={className}
  role="img"
  aria-label={title}
  aria-hidden={ariaHidden}
  width="64"
  height="64"
  viewBox="0 0 64 64"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <title>{title}</title>
  <use href="#logo-mark"></use>
</svg>
