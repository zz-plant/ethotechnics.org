---
import TaxonomyEntryPage from "../../components/TaxonomyEntryPage.astro";
import { taxonomyEntriesBySlug, type TaxonomyEntry } from "../../content/taxonomy";

const entry = taxonomyEntriesBySlug.get("delivery");

if (!entry) {
  throw new Error("Missing taxonomy entry for delivery.");
}
---

<TaxonomyEntryPage entry={entry as TaxonomyEntry} basePath="/delivery" eyebrow="Domain" />
