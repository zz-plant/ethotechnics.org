---
import TaxonomyEntryPage from "../../components/TaxonomyEntryPage.astro";
import { taxonomyEntriesBySlug, type TaxonomyEntry } from "../../content/taxonomy";

const entry = taxonomyEntriesBySlug.get("experience");

if (!entry) {
  throw new Error("Missing taxonomy entry for experience.");
}
---

<TaxonomyEntryPage entry={entry as TaxonomyEntry} basePath="/experience" eyebrow="Domain" />
