---
import TaxonomyEntryPage from "../../components/TaxonomyEntryPage.astro";
import { taxonomyEntries, type TaxonomyEntry } from "../../content/taxonomy";

export const prerender = true;

export function getStaticPaths() {
  return taxonomyEntries.map((entry) => ({
    params: { slug: entry.slug },
    props: { entry },
  }));
}

const { entry } = Astro.props as { entry: TaxonomyEntry };
---

<TaxonomyEntryPage
  entry={entry}
  basePath="/taxonomy"
  titleSuffix=" taxonomy â€” Ethotechnics Institute"
/>
