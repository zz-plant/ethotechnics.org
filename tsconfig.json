{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "skipLibCheck": true,
    "incremental": true,
    "tsBuildInfoFile": ".astro/tsconfig.tsbuildinfo",
    "types": [
      "astro/client",
      "vitest/globals",
      "@playwright/test",
      "jsdom",
      "node"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["coverage", "dist", "node_modules"]
}
